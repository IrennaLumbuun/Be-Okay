{
    "manifest_version" : 2,
    "name": "Stress Tracker",
    "description": "Track your stress level in pixels.",
    "version" : "1.0.0",
    "icons": {"128": "./favicon_io/favicon-128.png"},
    "browser_action":{
        "default_icon": "./favicon_io/favicon-128.png",
        "default_popup": "account.html"
    },
    "permissions": [
        "activeTab", 
        "https://www.gstatic.com/firebasejs/7.13.1/firebase-app.js",
        "https://www.gstatic.com/firebasejs/7.13.1/firebase-auth.js",
        "https://www.gstatic.com/firebasejs/7.13.1/firebase-firestore.js"
    ],
    "content_security_policy":"script-src 'self' https://www.gstatic.com; object-src 'self'"
}